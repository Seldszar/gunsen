{
  "name": "gunsen",
  "version": "0.0.0-development",
  "description": "Wrapper for Winston appending metadata to messages",
  "author": "Alexandre Breteau <contact@seldszar.fr>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/seldszar/gunsen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seldszar/gunsen.git"
  },
  "bugs": {
    "url": "https://github.com/seldszar/gunsen/issues"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "logging",
    "metadata",
    "winston",
    "wrapper"
  ],
  "engine": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "documentation": "documentation build lib/index.js --format=md --output=API.md",
    "lint": "eslint --format codeframe .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "documentation": "^8.1.2",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "semantic-release": "^15.12.3",
    "winston": "^3.1.0"
  },
  "peerDependencies": {
    "winston": ">=2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --format codeframe"
  }
}
