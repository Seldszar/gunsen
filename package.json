{
  "name": "gunsen",
  "version": "0.0.0-development",
  "description": "Wrapper for Winston appending metadata to messages",
  "author": "Alexandre Breteau <contact@seldszar.fr>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/seldszar/gunsen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seldszar/gunsen.git"
  },
  "bugs": {
    "url": "https://github.com/seldszar/gunsen/issues"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "logging",
    "metadata",
    "winston",
    "wrapper"
  ],
  "engine": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "documentation": "documentation build lib/index.js --format=md --output=API.md",
    "lint": "eslint --format codeframe .",
    "release": "semantic-release",
    "test": "jest"
  },
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "documentation": "^9.1.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "winston": "^3.2.1"
  },
  "peerDependencies": {
    "winston": ">=2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --format codeframe",
      "git add"
    ]
  }
}
